#include <platform.h>
#include <stdio.h>

// Ant position
struct Position {
    int x;
    int y;
};
typedef struct Position Position;

// Ant data structure
struct Ant {
    Position p;
    int foodCounter;
};
typedef struct Ant Ant;


int main(void){
    int grid[3][4] = {{10,0,1,7},{2,10,0,3},{6,8,7,6}};
    Ant ant1 = {{1,0}, 5};
    return 0;
}
